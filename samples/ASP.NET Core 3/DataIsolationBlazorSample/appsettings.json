{
  "Logging": {
    "IncludeScopes": false,
    "LogLevel": {
      "Default": "Warning"
    }
  },
  "TenantStrategy": {
    "RouteStrategy": false,
    "DefaultRoute": "{__tenant__=}/{controller=Home}/{action=Index}",
    "HostTemplate": "__tenant__.*"
  },
  "Finbuckle:MultiTenant:Stores:ConfigurationStore": {
    "Defaults": {
      "ConnectionString": "Data Source=Data/ToDoList.db"
    },
    "Tenants": [
      {
        "Id": "localhost",
        "Identifier": "localhost",
        "Name": "LocalHost",
        "ConnectionString": "Data Source=Data/local_ToDoList.db"
      },
      {
        "Id": "megacorp",
        "Identifier": "megacorp",
        "Name": "MegaCorp"
      },
      {
        "Id": "finbuckle",
        "Identifier": "finbuckle",
        "Name": "Finbuckle"
      },
      {
        "Id": "initech",
        "Identifier": "initech",
        "Name": "Initech LLC",
        "ConnectionString": "Data Source=Data/Initech_ToDoList.db"
      }
    ]
  },
  "AllowedHosts": "*"
}