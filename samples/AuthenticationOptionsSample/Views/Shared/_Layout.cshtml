<!DOCTYPE html>
<html>
<head>
    <title>@ViewBag.Title</title>
	<meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.1.2/css/bootstrap.min.css" integrity="sha384-Smlep5jCw/wG7hdkwQ/Z5nLIefveQRIY9nfy6xoR1uRYBtpZgI6339F5dgvm/e9B" crossorigin="anonymous">
</head>
<body>
	<nav class="navbar navbar-expand @ViewData["style"]">
        <div class="container">
            <a class="navbar-brand" href="#">@ViewData["Title"]</a>
            <div class="navbar-nav">
                <a asp-route-__tenant__="finbuckle" class="nav-item nav-link">Finbuckle (OpenID Connect)</a>
                <a asp-route-__tenant__="megacorp" class="nav-item nav-link">MegaCorp (Facebook)</a>
                <a asp-route-__tenant__="acme" class="nav-item nav-link">ACME (Google)</a>
                <a asp-route-__tenant__="initech" class="nav-item nav-link">Initech (Cookies)</a>
            </div>
        </div>
    </nav>
    <div class="container mt-5">
        @RenderBody()
    </div>
</body>
</html>
